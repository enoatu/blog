{
  "name": "enoatu-blog",
  "license": "MIT",
  "main": "n/a",
  "description": "Blog",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "fmt": "prettier --check --write 'src/**/*{.mjs,.js,.jsx,.ts,.tsx}'",
    "dev": "gatsby develop",
    "build": "gatsby build",
    "heroku-postbuild": "gatsby build",
    "serve": "gatsby serve",
    "setup": "npm install --no-save contentful-import yargs-parser axios chalk && node ./.setup.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "prettier --check --write 'src/**/*{.mjs,.js,.jsx,.ts,.tsx}'"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "lint-staged": "lint-staged",
      "pre-push": "yarn build"
    }
  },
  "dependencies": {
    "dat.gui": "^0.7.7",
    "eslint-plugin-react": "^7.25.1",
    "gatsby": "^2.19.1",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-image": "^1.13.0",
    "gatsby-plugin-provide-react": "^1.0.2",
    "gatsby-plugin-react-helmet": "^4.13.0",
    "gatsby-plugin-sharp": "^3.13.0",
    "gatsby-source-contentful": "^4.6.4",
    "gatsby-transformer-remark": "^4.10.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.2",
    "lint-staged": "^11.0.0",
    "lodash": "^4.17.21",
    "prettier": "^2.3.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.0",
    "ssr-window": "^4.0.0"
  },
  "devDependencies": {
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-typography": "^3.13.0",
    "gatsby-transformer-sharp": "^3.13.0",
    "react-typography": "^0.16.20",
    "sharp": "0.29.0",
    "typography": "^0.16.21"
  }
}
